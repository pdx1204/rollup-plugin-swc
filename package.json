  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "rollup": "^3.15.0",
    "rome": "^11.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@swc/core": "^1.3.35"
  },
  "peerDependencies": {
    "rollup": ">=3.15.0",
    "@swc/core": ">=1.3.35"
  },
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiuqfang/rollup-plugin-swc.git"
  },
